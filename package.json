{
  "name": "budgetzero",
  "version": "0.1.7",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit --silent",
    "test-watch:unit": "vue-cli-service test:unit --watch --silent",
    "test:debug": "node --inspect-brk node_modules/.bin/vue-cli-service test:unit --runInBand --watchAll",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "electron:build": "vue-cli-service electron:build --publish always",
    "electron:generate-icons": "electron-icon-builder --input=./public/icon.png --output=build --flatten",
    "electron:serve": "vue-cli-service electron:serve",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "test:cypress": "cypress run"
  },
  "main": "background.js",
  "dependencies": {
    "@sweetalert2/theme-borderless": "^4.0.1",
    "banking": "^1.2.0",
    "core-js": "^3.8.1",
    "electron-updater": "^4.3.5",
    "file-saver": "^2.0.5",
    "firebase": "^8.2.1",
    "firebaseui": "^4.7.1",
    "is-electron": "^2.2.0",
    "jsonschema": "^1.4.0",
    "lodash": "^4.17.20",
    "material-design-icons-iconfont": "^6.1.0",
    "moment": "^2.29.1",
    "pouch-vue": "^0.3.5",
    "pouchdb": "^7.2.2",
    "pouchdb-authentication": "^1.1.3",
    "pouchdb-browser": "^7.2.2",
    "pouchdb-erase": "^1.0.2",
    "pouchdb-find": "^7.2.2",
    "pouchdb-live-find": "^0.4.0",
    "pouchdb-load": "^1.4.6",
    "sortablejs": "^1.10.2",
    "sweetalert": "^2.1.2",
    "validator": "^13.5.2",
    "vue": "^2.6.12",
    "vue-csv-import": "^3.3.5",
    "vue-json-tree-view": "^2.1.6",
    "vue-lodash": "^2.1.2",
    "vue-moment": "^4.1.0",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.4.9",
    "vue-select": "^3.11.2",
    "vue-sweetalert2": "^4.1.1",
    "vue-uuid": "^2.0.2",
    "vue2-filters": "^0.11.1",
    "vuedraggable": "^2.24.3",
    "vuefire": "^2.2.5",
    "vuejs-auto-complete": "^0.9.0",
    "vuejs-datepicker": "^1.6.2",
    "vuelidate": "^0.7.6",
    "vuetify": "^2.4.0",
    "vuex": "^3.6.0",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.9",
    "@vue/cli-plugin-eslint": "^4.5.9",
    "@vue/cli-plugin-router": "^4.5.9",
    "@vue/cli-plugin-unit-jest": "^4.5.9",
    "@vue/cli-plugin-vuex": "^4.5.9",
    "@vue/cli-service": "^4.5.9",
    "@vue/eslint-config-airbnb": "^5.3.0",
    "@vue/test-utils": "^1.1.2",
    "babel-eslint": "^10.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "electron": "^11.1.1",
    "electron-builder": "^22.9.1",
    "electron-icon-builder": "^2.0.1",
    "eslint": "^7.16.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-vue": "^7.3.0",
    "lint-staged": "^10.5.3",
    "node-sass": "^5.0.0",
    "sass": "^1.30.0",
    "sass-loader": "^10.1.0",
    "vue-cli-plugin-electron-builder": "^2.0.0-rc.5",
    "vue-cli-plugin-vuetify": "^2.0.8",
    "vue-template-compiler": "^2.6.12",
    "vuetify-loader": "^1.6.0",
    "yarn-upgrade-all": "^0.5.4"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/recommended",
      "eslint:recommended",
      "@vue/airbnb"
    ],
    "rules": {
      "func-names": "off",
      "no-console": "off",
      "linebreak-style": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "gitHooks": {
    "post-push": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
