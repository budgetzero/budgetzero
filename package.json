{
  "name": "budgetzero",
  "version": "0.1.20",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit --silent",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "electron:build": "vue-cli-service electron:build --publish always",
    "electron:generate-icons": "electron-icon-builder --input=./public/icon.png --output=build --flatten",
    "electron:serve": "vue-cli-service electron:serve",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "test-watch:unit": "vue-cli-service test:unit --watch --silent",
    "test:cypress": "cypress run",
    "test:debug": "node --inspect-brk node_modules/.bin/vue-cli-service test:unit --runInBand --watchAll"
  },
  "main": "background.js",
  "dependencies": {
    "@sweetalert2/theme-borderless": "^4.0.5",
    "@vue/cli": "^4.5.15",
    "banking": "^1.2.0",
    "chart.js": "^3.3.2",
    "core-js": "^3.14.0",
    "electron-updater": "^4.3.9",
    "file-saver": "^2.0.5",
    "firebase": "^8.6.8",
    "firebaseui": "^4.8.0",
    "is-electron": "^2.2.0",
    "jsonschema": "^1.4.0",
    "lodash": "^4.17.21",
    "material-design-icons-iconfont": "^6.1.0",
    "moment": "^2.29.1",
    "pouch-vue": "^0.3.5",
    "pouchdb": "^7.2.2",
    "pouchdb-authentication": "^1.1.3",
    "pouchdb-browser": "^7.2.2",
    "pouchdb-erase": "^1.0.2",
    "pouchdb-find": "^7.2.2",
    "pouchdb-live-find": "^0.4.0",
    "pouchdb-load": "^1.4.6",
    "sortablejs": "^1.13.0",
    "sweetalert": "^2.1.2",
    "validator": "^13.6.0",
    "vue": "^2.6.14",
    "vue-csv-import": "^3.3.7",
    "vue-json-tree-view": "^2.1.6",
    "vue-lodash": "^2.1.2",
    "vue-moment": "^4.1.0",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.5.1",
    "vue-select": "^3.11.2",
    "vue-sweetalert2": "^5.0.0",
    "vue-uuid": "^2.0.2",
    "vue2-filters": "^0.13.0",
    "vuedraggable": "^2.24.3",
    "vuefire": "^2.2.5",
    "vuejs-auto-complete": "^0.9.0",
    "vuejs-datepicker": "^1.6.2",
    "vuelidate": "^0.7.6",
    "vuetify": "^2.5.4",
    "vuex": "^3.6.2",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "4.5.15",
    "@vue/cli-plugin-e2e-cypress": "4.5.15",
    "@vue/cli-plugin-eslint": "4.5.15",
    "@vue/cli-plugin-router": "4.5.15",
    "@vue/cli-plugin-unit-jest": "4.5.15",
    "@vue/cli-plugin-vuex": "4.5.15",
    "@vue/cli-service": "4.5.15",
    "@vue/eslint-config-airbnb": "5.3.0",
    "@vue/test-utils": "1.2.2",
    "babel-eslint": "10.1.0",
    "babel-plugin-transform-remove-console": "6.9.4",
    "electron": "15.3.0",
    "electron-builder": "22.13.1",
    "electron-devtools-installer": "3.2.0",
    "electron-icon-builder": "2.0.1",
    "eslint": "8.1.0",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-jest": "25.2.2",
    "eslint-plugin-vue": "8.0.3",
    "lint-staged": "11.2.6",
    "node-sass": "6.0.1",
    "sass": "1.80.3",
    "sass-loader": "10.2.0",
    "vue-cli-plugin-electron-builder": "2.1.1",
    "vue-cli-plugin-vuetify": "2.4.3",
    "vue-template-compiler": "2.6.14",
    "vuetify-loader": "1.7.3",
    "yarn-upgrade-all": "0.5.4"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/recommended",
      "eslint:recommended",
      "@vue/airbnb"
    ],
    "rules": {
      "func-names": "off",
      "no-console": "off",
      "linebreak-style": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "gitHooks": {
    "post-push": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BudgetZero/BudgetZero"
  }
}
